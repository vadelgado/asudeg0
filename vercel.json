{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.5.4"
    },
    {
      "src": "api/index.php",
      "use": "vercel-php@0.5.4"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Alianza Sure√±a",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://asudeg0.vercel.app/",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "array",
    "VIEW_COMPILED_PATH": "/tmp/views"
  }
}
